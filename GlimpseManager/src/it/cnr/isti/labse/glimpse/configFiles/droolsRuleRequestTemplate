        import it.cnr.isti.labse.glimpse.event.GlimpseBaseEventChoreos;
        import it.cnr.isti.labse.glimpse.manager.ResponseDispatcher;
        import it.cnr.isti.labse.glimpse.utils.NotifierUtils;
        import it.cnr.isti.labse.glimpse.impl.ServiceLocatorImpl;
	import it.cnr.isti.labse.glimpse.impl.RuleTemplateEnum;
        
        declare GlimpseBaseEventChoreos
	        @role( event )
	        @timestamp( timeStamp )
        end
        
		rule "de"
		no-loop
		salience 999
		dialect "java"
		when
			$aEvent : GlimpseBaseEventChoreos(this.choreographySource == "CH1", this.isConsumed == false, this.isException == false, this.getEventData == "SLA Violation");
			$bEvent : GlimpseBaseEventChoreos(this.choreographySource == "CH1", this.isConsumed == false, this.isException == false, this.getEventData == "Infrastructure Violation", this after $aEvent);
		then
			$aEvent.setConsumed(true);
			$bEvent.setConsumed(true);
			update( $aEvent );
			retract( $aEvent );
			update( $bEvent );
			retract( $bEvent );
			ResponseDispatcher.LogViolation(drools.getRule().getName(),"auto_generated_rule","SLA and Infrastructure violation occurred");
		end
